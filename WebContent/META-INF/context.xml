<?xml version="1.0" encoding="UTF-8"?>
<Context path="/acomersedijo">
    
    <Resource
        name="jdbc/Acomersedijo"
        auth="Container"
        type="javax.sql.DataSource"
        maxActive="20"
        maxIdle="30"
